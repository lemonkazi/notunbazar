<?xml version="1.0"?>
<layout version="0.1.0">
    <default>
        <reference name="footer_links">
            <action method="addLink" translate="label title" module="abac_classified">
                <label>Sell</label>
                <url>mgt</url>
                <title>News</title>
                <prepare>true</prepare>
            </action>
        </reference>
    </default>

    <abac_classified_test_test translate="label">
        <label>News Page</label>
        <reference name="root">
            <action method="setTemplate">
                <template>page/2columns-right.phtml</template>
            </action>
            <action method="setHeaderTitle" translate="title" module="abac_classified">
                <title>Site News</title>
            </action>
        </reference>
        <reference name="content">
            <block type="abac_classified/sell" name="sell.product" template="ABAC/Classified/sell.phtml">
                <!--<block type="page/html_pager" name="news.list.pager" as="news_list_pager" /> -->
            </block>
        </reference>
    </abac_classified_test_test>
</layout>